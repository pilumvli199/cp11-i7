# .env example for Indian Market Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_chat_id
SMARTAPI_API_KEY=your_api_key
SMARTAPI_API_SECRET=your_api_secret
SMARTAPI_CLIENT_CODE=your_client_code_here
# Option A: MPIN (preferred)
SMARTAPI_MPIN=your_mpin_here
# Option B: Password + TOTP (fallback)
SMARTAPI_PASSWORD=your_password_here
SMARTAPI_TOTP_SECRET=your_totp_base32_here
OPENAI_API_KEY=your_openai_key_here
OPENAI_MODEL=gpt-4o-mini
POLL_INTERVAL=120